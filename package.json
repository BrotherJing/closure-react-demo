{
    "name": "closure-react-demo",
    "version": "0.0.1",
    "scripts": {
        "server-start": "./node_modules/http-server/bin/http-server",
        "build": "rm -f ./dist/* && java -jar node_modules/google-closure-compiler/compiler.jar --flagfile closure.conf",
        "build-debug": "rm -f ./dist/* && java -jar node_modules/google-closure-compiler/compiler.jar --flagfile closure.conf --debug --formatting PRETTY_PRINT",
        "build-native": "rm -f ./dist/* && java -jar node_modules/google-closure-compiler-linux/compiler --flagfile closure.conf"
    },
    "dependencies": {
        "google-closure-library": "^20180910.0.0",
        "http-server": "^0.11.1",
        "react": "~16.4.0",
        "react-dom": "~16.4.0"
    },
    "devDependencies": {
        "google-closure-compiler": "^20181008.0.0"
    }
}
