{
    "name": "closure-react-demo",
    "version": "0.0.1",
    "scripts": {
        "build:deps": "bash ./scripts/build-deps.sh",
        "build:webpack-dev": "cross-env BABEL_ENV=dev NODE_ENV=development node_modules/.bin/webpack --progress --watch",
        "build:webpack-prod": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.prod.config.js --progress",
        "build": "run-s build:deps build:webpack-prod",
        "build-dev": "run-s build:deps build:webpack-dev",
        "build:lib": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.lib.config.js --progress",
        "test:watch": "karma start karma.config.js --auto-watch",
        "test:unit": "NODE_ENV=test karma start karma.config.js --single-run",
        "test:unit-ci": "karma start karma.ci.js --single-run"
    },
    "dependencies": {
        "@material-ui/core": "3.2.0",
        "classnames": "^2.2.6",
        "google-closure-deps": "^20180910.1.0",
        "google-closure-library": "^20180910.0.0",
        "react": "~16.4.0",
        "react-dom": "~16.4.0",
        "redux": "^4.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.5",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-destructuring": "^7.1.3",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.4.3",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.4",
        "bfj": "^6.1.1",
        "chai": "^4.2.0",
        "closure-webpack-plugin": "^1.1.1",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.8.0",
        "eslint": "^5.6.0",
        "eslint-config-closure-base": "^0.1.1",
        "eslint-config-closure-es5": "^0.1.1",
        "eslint-config-react-app": "^3.0.5",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-closure": "^0.1.1",
        "eslint-plugin-flowtype": "^2.50.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.11.1",
        "extract-loader": "^3.0.0",
        "file-loader": "^2.0.0",
        "fs-extra": "^7.0.0",
        "google-closure-compiler": "^20181008.0.0",
        "karma": "^4.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-mocha": "^1.3.0",
        "karma-webpack": "^3.0.5",
        "mocha": "^5.2.0",
        "node-sass": "^4.10.0",
        "npm-run-all": "^4.1.3",
        "postcss-loader": "^3.0.0",
        "puppeteer": "^1.11.0",
        "react-dev-utils": "^6.1.1",
        "sass-loader": "^7.1.0",
        "sinon": "^7.2.3",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.x",
        "webpack-merge-and-include-globally": "^2.1.14"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "rules": {
            "no-undef": 0,
            "react/prop-types": 1
        }
    }
}
