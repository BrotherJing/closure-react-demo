{
    "name": "closure-react-demo",
    "version": "0.0.1",
    "scripts": {
        "start": "run-s build:deps start:devserver",
        "start:devserver": "webpack-dev-server",
        "build": "run-s build:deps build:webpack",
        "build:webpack": "node_modules/.bin/webpack",
        "build:deps": "./build-deps.sh",
        "build:gcc": "java -jar node_modules/google-closure-compiler/compiler.jar --flagfile closure.conf",
        "build:debug": "java -jar node_modules/google-closure-compiler/compiler.jar --flagfile closure.conf --debug --formatting PRETTY_PRINT"
    },
    "dependencies": {
        "@material/button": "^0.41.0",
        "@material/react-button": "^0.6.2",
        "@material/ripple": "^0.41.0",
        "classnames": "^2.2.6",
        "google-closure-deps": "^20180910.1.0",
        "google-closure-library": "^20180910.0.0",
        "react": "~16.4.0",
        "react-dom": "~16.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.5",
        "@babel/plugin-transform-destructuring": "^7.1.3",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.4",
        "closure-webpack-plugin": "^1.1.1",
        "css-loader": "^1.0.1",
        "extract-loader": "^3.0.0",
        "file-loader": "^2.0.0",
        "google-closure-compiler": "^20181008.0.0",
        "node-sass": "^4.10.0",
        "npm-run-all": "^4.1.3",
        "sass-loader": "^7.1.0",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.x"
    }
}
