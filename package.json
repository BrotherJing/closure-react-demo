{
    "name": "closure-react-demo",
    "version": "0.0.1",
    "scripts": {
        "build:deps": "bash ./build-deps.sh",
        "build:webpack-dev": "cross-env BABEL_ENV=dev NODE_ENV=development node_modules/.bin/webpack --progress --watch",
        "build:webpack-prod": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.prod.config.js --progress",
        "build": "run-s build:deps build:webpack-prod",
        "build-dev": "run-s build:deps build:webpack-dev",
        "build:lib": "cross-env NODE_ENV=development node_modules/.bin/webpack --config webpack.lib.config.js --progress"
    },
    "dependencies": {
        "@material-ui/core": "3.2.0",
        "classnames": "^2.2.6",
        "google-closure-deps": "^20180910.1.0",
        "google-closure-library": "^20180910.0.0",
        "react": "~16.4.0",
        "react-dom": "~16.4.0",
        "redux": "^4.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.5",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-destructuring": "^7.1.3",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.4.3",
        "babel-loader": "^8.0.4",
        "closure-webpack-plugin": "^1.1.1",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
        "extract-loader": "^3.0.0",
        "file-loader": "^2.0.0",
        "google-closure-compiler": "^20181008.0.0",
        "node-sass": "^4.10.0",
        "npm-run-all": "^4.1.3",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "uglifyjs-3-webpack-plugin": "^1.2.4",
        "umd-compat-loader": "^2.1.1",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.x"
    }
}
